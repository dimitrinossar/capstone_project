{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/react-src/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "filesystem.readDirectory"
  ],
  "modes": {
    "window": {
      "title": "ga-capstone-project-desktop",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200
    }
  },
  "cli": {
    "binaryName": "ga-capstone-project-desktop",
    "resourcesPath": "/react-src/dist/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "4.10.0",
    "clientVersion": "3.8.2",
    "frontendLibrary": {
      "patchFile": "/react-src/index.html",
      "devUrl": "http://localhost:5173"
    }
  }
}